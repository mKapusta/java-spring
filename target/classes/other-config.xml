<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="monBean" class="com.example.BeanClass">
        <property name="randomValue" value="test"></property>
    </bean>

    <bean id="monBean2" class="com.example.BeanClass">
        <property name="randomValue" value="test2"></property>
    </bean>


    <bean id="upperBean" class="com.example.UpperBeanClass">
        <property name="beanClass" ref="monBean2"></property>
    </bean>


</beans>